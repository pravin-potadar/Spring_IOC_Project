<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="a" class="com.nt.Address">
		<property name="city" value="Pune" />
		<property name="state" value="MH" />
	</bean>

	<bean id="e" class="com.nt.Employee">
		<property name="id" value="111" />
		<property name="name" value="Pravin" />
		<property name="sal" value="1000" />
		<property name="adr" ref="a" />
	</bean>
	
	<bean id="a1" class="com.nt.Address">
		<property name="city" value="Pune" />
		<property name="state" value="MH" />
	</bean>

	<bean id="e1" class="com.nt.Employee">
		<property name="id" value="123" />
		<property name="name" value="pratik" />
		<property name="sal" value="1000" />
		<property name="adr" ref="a1" />
	</bean>

</beans>